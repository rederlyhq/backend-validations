{
    "type": "object",
    "properties": {
        "firstName": {
            "type": "string",
            "description": "User's first name.",
            "example": "Tommy"
        },
        "lastName": {
            "type": "string",
            "description": "User's last name.",
            "example": "Turner"
        },
        "email": {
            "type": "string",
            "format": "email",
            "description": "The email to register with.",
            "example": "test@rederly.com"
        },
        "password": {
            "type": "string",
            "description": "The password for the given email.",
            "example": "password",
            "minLength": 4,
            "maxLength": 26
        }
    },
    "required": ["email", "password", "firstName", "lastName"],
    "additionalProperties": false
}