                                "example": "1994-01-26T14:30:00.000Z",
                                "maxLength": 30
                            },
                            "updatedAt": {
                                "type": "string",
                                "format": "date-time",
                                "tsType": "Date",
                                "description": "When the grade was updated.",
                                "example": "1994-01-26T14:30:00.000Z",
                                "maxLength": 30
