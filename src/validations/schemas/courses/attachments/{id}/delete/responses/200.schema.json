{
    "type": "object",
    "properties": {
        "message": {
            "type": "string"
        },
        "status": {
            "type": "string"
        },
        "statusCode": {
            "type": "number",
            "enum": [200],
            "example": 200
        },
        "data": {
            "type": "object",
            "additionalProperties": false,
            "required": ["updatedCount", "updatedRecords"],
            "properties": {
                "updatedCount": {
                    "type": "number",
                    "example": 1,
                    "description": "TODO"
                },
                "updatedRecords": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "required": ["id", "cloudFilename", "userLocalFilename", "active", "createdAt", "updatedAt"],
                        "properties": {
                            "id": {
                                "type": "number",
                                "description": "The rederly identifier for the attachment",
                                "example": 14573
                            },
                            "cloudFilename": {
                                "type": "string",
                                "description": "The cloud filename of the attachment",
                                "example": "abc-def-ghi"
                            },
                            "userLocalFilename": {
                                "type": "number",
                                "description": "The filename the user had for the attachment",
                                "example": "tom.jpg"
                            },
                            "active": {
                                "type": "boolean",
                                "description": "Whether or not the attachment was deleted",
                                "example": false
                            },
                            "createdAt": {
                                "type": "string",
                                "format": "date-time",
                                "tsType": "Date",
                                "description": "When the attachment was created.",
                                "example": "1994-01-26T14:30:00.000Z",
                                "maxLength": 30
                            },
                            "updatedAt": {
                                "type": "string",
                                "format": "date-time",
                                "tsType": "Date",
                                "description": "When the attachment was updated.",
                                "example": "1994-01-26T14:30:00.000Z",
                                "maxLength": 30
                            }
                        }
                    }
                }
            }
        }
    },
    "required": ["data", "statusCode", "status", "message"],
    "additionalProperties": false
}